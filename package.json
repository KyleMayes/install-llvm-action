{
  "name": "install-llvm-action",
  "version": "2.0.0",
  "description": "A GitHub Action for downloading and installing LLVM and Clang binaries.",
  "author": "Kyle Mayes <kyle@mayeses.com>",
  "license": "Apache-2.0",
  "main": "dist/main.js",
  "scripts": {
    "format": "prettier --write *.ts",
    "generate": "ts-node generate.ts",
    "build": "parcel build main.ts",
    "test": "ts-node test.ts test"
  },
  "dependencies": {
    "@actions/core": "~1.10.0",
    "@actions/exec": "~1.1.1",
    "@actions/io": "~1.1.3",
    "@actions/tool-cache": "~2.0.1"
  },
  "devDependencies": {
    "@octokit/rest": "^20",
    "@types/lodash": "^4",
    "@types/node": "~20",
    "lodash": "^4",
    "parcel": "^2",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "targets": {
    "main": {
      "context": "node",
      "engines": { "node": "20" },
      "includeNodeModules": true
    }
  }
